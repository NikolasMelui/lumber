{
  "name": "lumber-cli",
  "version": "1.8.1",
  "description": "Generate the admin micro-service of your application",
  "main": "dist/lumber.js",
  "scripts": {
    "test": "ava",
    "prebuild": "rm -rf dist/*",
    "build": "babel --copy-files --source-maps inline --retain-lines --ignore templates/*,node_modules . -d dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "lumber": "dist/lumber.js"
  },
  "author": "Sandro Munda",
  "license": "GPL-3.0",
  "dependencies": {
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "expand-home-dir": "0.0.3",
    "github": "^6.0.4",
    "inquirer": "^1.2.2",
    "jsonapi-serializer": "^3.4.1",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "mysql": "^2.11.1",
    "mysql2": "1.4.2",
    "pg": "^6.1.0",
    "sequelize": "git://github.com/ForestAdmin/sequelize.git#feature/support-schema",
    "sqlite3": "3.1.13",
    "superagent": "^2.3.0",
    "superagent-promise": "^1.1.0",
    "tedious": "^1.14.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-object-assign"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.0"
          }
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/sample-project/**/*.js"
    ]
  }
}
